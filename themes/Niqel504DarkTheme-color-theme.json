{
	"name": "Niqel504DarkTheme",
		"colors": {
			"activityBar.activeBackground": "#132833", // Color de fondo del icono activo en la barra de actividades.
			"activityBar.background": "#211c22", // Color de fondo de la barra de actividades (barra lateral izquierda).
			"activityBar.border": "#00ddff", // Color del borde de la barra de actividades.
			"activityBar.foreground": "#00ddff", // Color de los iconos en la barra de actividades.
			"activityBarBadge.background": "#132833", // Color de fondo de los badges (iconos de notificación) en la barra de actividades.
			"activityBarBadge.foreground": "#00ddff", // Color del texto o icono en los badges de la barra de actividades.
			"button.background": "#132833", // Color de fondo de los botones.
			"button.foreground": "#00ddff", // Color del texto en los botones.
			"button.hoverBackground": "#154450", // Color de fondo de los botones cuando se pasa el ratón sobre ellos.
			"descriptionForeground": "#6aa3bb", // Color del texto en descripciones.
			"disabledForeground": "#198b99", // Cambia el color del texto de los elementos deshabilitados en toda la interfaz.
			"dropdown.background": "#1c1b22", // Color de fondo de los menús desplegables (dropdowns).
			"dropdown.border": "#00ddff", // Color del borde de los menús desplegables.
			"dropdown.foreground": "#fff6ee", // Color del texto en los menús desplegables.
			"editor.background": "#1c1b22", // Color de fondo del editor de texto.
			"editor.foreground": "#eeffff", // Color del texto en el editor.
			"editorGroupHeader.tabsBackground": "#132833", // Color de fondo de la barra de pestañas que siempre es visible.
			"editorLineNumber.activeForeground": "#00ddff", // Color del número de línea activa en el editor.
			"focusBorder": "#00ddff", // Color del borde de los elementos cuando tienen el foco (enfoque).
			"input.background": "#211c22", // Color de fondo de los campos de entrada de texto.
			"input.border": "#00ddff", // Color del borde de los campos de entrada de texto.
			"input.foreground": "#6aa3bb", // Color del texto en los campos de entrada.
			"input.placeholderForeground": "#6aa3bb", // Color del texto del placeholder en los campos de entrada.
			"list.activeSelectionBackground": "#132833", // Color de fondo del elemento seleccionado activamente en una lista.
			"list.activeSelectionForeground": "#00ddff", // Color del texto del elemento seleccionado activamente en una lista.
			"list.focusOutline": "#00ddff", // Color del borde de los elementos en la lista cuando tienen el foco.
			"list.hoverBackground": "#132833", // Color de fondo de los elementos de lista cuando se pasa el ratón sobre ellos.
			"list.hoverForeground": "#00ddff", // Color del texto de los elementos de lista cuando se pasa el ratón sobre ellos.
			"list.inactiveSelectionBackground": "#132833", // Color de fondo del elemento seleccionado inactivamente en una lista.
			"list.inactiveSelectionForeground": "#00ddff", // Color del texto del elemento seleccionado inactivamente en una lista.
			"list.inactiveSelectionIconForeground": "#00ddff", // Color del icono en el elemento seleccionado inactivamente en una lista.
			"menubar.selectionBackground": "#132833", // Color de fondo de los elementos en la barra de menú cuando se pasa el ratón sobre ellos.
			"menu.background": "#211c22", // Color de fondo de los menús emergentes (contextuales).
			"menu.foreground": "#00ddff", // Color del texto en los menús emergentes.
			"menu.selectionBackground": "#132833", // Color de fondo del elemento seleccionado de un menú emergente.
			"menu.selectionBorder": "#00ddff", // Color del borde del elemento seleccionado de un menú emergente.
			"menu.selectionForeground": "#00ddff", // Color del texto en el elemento seleccionado de un menú emergente.
			"menu.separatorBackground": "#6aa3bb", // Color de las líneas divisorias en los menús emergentes.
			"outputView.background": "#1c1b22", // Color de fondo específico para la vista "Output".
			"panel.background": "#1c1b22", // Color de fondo de todo el contenido del panel.
			"panel.border": "#132833", // Borde del panel.
			"panelSection.dropBackground": "#132833", // Color de fondo cuando un panel se arrastra sobre otro (drag-and-drop).
			"panelSectionHeader.background": "#132833", // Color de fondo del encabezado del panel, donde están los textos (similar al fondo de las pestañas inactivas).
			"panelSectionHeader.foreground": "#00ddff", // Color del texto en el encabezado del panel.
			"panelTitle.activeBorder": "#00ddff", // Borde inferior del título del panel activo.
			"panelTitle.activeForeground": "#00ddff", // Color del texto en la barra inferior cuando los paneles están activos, utilizando cian.
			"panelTitle.inactiveForeground": "#6aa3bb", // Color del texto en el título del panel inactivo.
			"sideBar.background": "#211C22", // Color de fondo de la barra lateral.
			"sideBar.border": "#00ddff", // Color del borde de la barra lateral.
			"sideBar.foreground": "#6aa3bb", // Color del texto en la barra lateral.
			"sideBarSectionHeader.background": "#132833", // Color de fondo de los encabezados de secciones en la barra lateral.
			"sideBarSectionHeader.foreground": "#00ddff", // Color del texto en los encabezados de secciones en la barra lateral.
			"sideBarTitle.background": "#132833", // Color de fondo de los títulos en la barra lateral.
			"sideBarTitle.foreground": "#00ddff", // Color del texto de los títulos en la barra lateral.
			"statusBar.background": "#132833", // Color de fondo de la barra de estado (parte inferior del editor).
			"statusBar.border": "#00ddff", // Color del borde de la barra de estado.
			"statusBar.foreground": "#00ddff", // Color del texto en la barra de estado.
			"tab.activeBackground": "#1c1b22", // Color de fondo de la pestaña activa.
			"tab.activeBorderTop": "#00ddff", // Color del borde superior de la pestaña activa.
			"tab.activeForeground": "#00ddff", // Color del texto en la pestaña activa.
			"tab.inactiveBackground": "#132833", // Color de fondo de las pestañas inactivas.
			"tab.inactiveForeground": "#6aa3bb", // Color del texto en las pestañas inactivas.
			"terminal.background": "#1c1b22", // Color de fondo de la terminal, igual al fondo del editor.
			"terminal.border": "#00ddff", // Color del borde de la terminal.
			"terminal.foreground": "#eeffff", // Color del texto en la terminal, igual que en el editor.
			"textLink.foreground": "#00ddff", // Color del texto de los enlaces.
			"titleBar.activeBackground": "#211C22", // Color de fondo de la barra de título cuando la ventana está activa.
			"titleBar.activeForeground": "#00ddff" // Color del texto en la barra de título cuando la ventana está activa.
		},

